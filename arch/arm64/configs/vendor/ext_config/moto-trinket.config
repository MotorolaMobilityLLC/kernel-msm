CONFIG_CPU_FREQ_STAT=y
CONFIG_CPU_FREQ_TIMES=y
CONFIG_USB_UAS=y
CONFIG_USB_RTL8152=y
CONFIG_SERIAL_MSM_GENI_CONSOLE=y
