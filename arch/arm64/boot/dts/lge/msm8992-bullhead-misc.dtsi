&soc {
    gpio_keys {
        compatible = "gpio-keys";
        input-name = "gpio-keys";

        vol_up {
            label = "volume_up";
            gpios = <&pm8994_gpios 3 0x1>;
            linux,input-type = <1>;
            linux,code = <115>;
            gpio-key,wakeup;
            debounce-interval = <15>;
        };
    };
};

&pmi8994_haptics {
	status = "okay";
	qcom,actuator-type = "lra";
	qcom,vmax-mv = <2400>;
	qcom,ilim-ma = <400>;
};
