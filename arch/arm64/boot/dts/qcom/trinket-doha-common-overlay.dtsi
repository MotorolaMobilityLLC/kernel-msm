#include "trinket-moto-common-overlay.dtsi"
#include "doha-panel.dtsi"
#include "doha-dtv.dtsi"
#include "trinket-doha-audio-overlay.dtsi"
#include "trinket-doha-touch-overlay.dtsi"
#include "trinket-doha-camera-overlay.dtsi"
#include "trinket-doha-haptic-overlay.dtsi"
#include "trinket-doha-fps-overlay.dtsi"
#include "trinket-doha-nfc-overlay.dtsi"
#include "trinket-doha-sensor-overlay.dtsi"

&usb_qmp_phy {
	status = "disabled";
};

&usb0 {
	dwc3@4e00000 {
		usb-phy = <&qusb_phy0>, <&usb_nop_phy>;
		maximum-speed = "high-speed";
	};
};

&pmi632_rgb {
	red {
		label = "charging";
	};
};

&mtp_batterydata {
	qcom,batt-id-range-pct = <15>;
	df-serialnum = "SB18C52858";
	#include "qg-batterydata-kd40-scud-4000mah.dtsi"
};

&pmi632_qg {
	qcom,battery-data = <&mtp_batterydata>;
	qcom,qg-iterm-ma = <100>;
	qcom,hold-soc-while-full;
	qcom,linearize-soc;
	qcom,qg-use-s7-ocv;
};

&pmi632_charger {
	qcom,battery-data = <&mtp_batterydata>;
};

&soc {
	qcom,tz_gpio {
		compatible = "qcom,tz_gpio";
		tlmm-tz = <30>,
			<31>,
			<32>,
			<33>;
	};
};

&pm6125_vadc {
	rf_pa0_therm {
		status = "disabled";
	};
	pa_therm0 {
		reg = <ADC_AMUX_THM1_PU2>;
		label = "pa_therm0";
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
	};

	emmc_ufs_therm {
		status = "disabled";
	};
	msm_therm {
		reg = <ADC_GPIO3_PU2>;
		label = "msm_therm";
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
	};
};

&pm6125_adc_tm {
	rf_pa0_therm {
		status = "disabled";
	};
};

&pmi632_vadc {
	skin_therm {
		status = "disabled";
	};
	chg_therm {
		reg = <ADC_GPIO2_PU2>;
		label = "chg_therm";
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
	};
};

&pmi632_adc_tm {
	skin_therm {
		status = "disabled";
	};
};

&thermal_zones {
	rf-pa0-therm-adc {
		status = "disabled";
	};
	pa_therm0 {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-has-hwmon;
		thermal-governor = "user_space";
		thermal-sensors = <&pm6125_adc_tm ADC_AMUX_THM1_PU2>;
		trips {
			active-config0 {
				temperature = <125000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};

	quiet-therm-adc {
		status = "disabled";
	};
	quiet_therm {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-has-hwmon;
		thermal-governor = "user_space";
		thermal-sensors = <&pm6125_adc_tm ADC_AMUX_THM2_PU2>;
		trips {
			active-config0 {
				temperature = <125000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};

	xo-therm-adc {
		status = "disabled";
	};
	xo_therm {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-has-hwmon;
		thermal-governor = "user_space";
		thermal-sensors = <&pm6125_adc_tm ADC_XO_THERM_PU2>;
		trips {
			active-config0 {
				temperature = <125000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};

	conn-therm-adc {
		status = "disabled";
	};

	skin-therm-adc {
		status = "disabled";
	};

	chg_therm {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-has-hwmon;
		thermal-governor = "user_space";
		thermal-sensors = <&pmi632_adc_tm ADC_GPIO2_PU2>;
		trips {
			active-config0 {
				temperature = <125000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};

	camera-ftherm-adc {
		status = "disabled";
	};

	emmc-ufs-therm-adc {
		status = "disabled";
	};

	msm_therm {
		polling-delay-passive = <0>;
		polling-delay = <5000>;
		thermal-has-hwmon;
		thermal-governor = "user_space";
		thermal-sensors = <&pm6125_adc_tm_iio ADC_GPIO3_PU2>;
		trips {
			active-config0 {
				temperature = <125000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};
};
