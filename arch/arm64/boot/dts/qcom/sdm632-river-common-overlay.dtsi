				0x83 0x88
				0xc7 0x8c
				0x14 0x9c
				0x30 0x08
				0x79 0x0c
				0x21 0x10
				0x00 0x90
				0x9f 0x1c
				0x00 0x18>;
	qcom,tune-factory-override = <0xC783B1C0>;
	qcom,tune2-host-override = <0xb1>;
};

