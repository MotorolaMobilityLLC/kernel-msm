};

&sdhc_2 {
	cd-gpios = <&tlmm 133 0x0>;
	qcom,sdr104-wa;
};

&pm8953_vadc {
	pinctrl-0 = <&chg_therm_default &msm_therm_default>;
	chan@11 {
		label = "chg_therm";
		reg = <0x11>;
		qcom,decimation = <0>;
		qcom,pre-div-channel-scaling = <0>;
		qcom,calibration-type = "ratiometric";
		qcom,scale-function = <2>;
		qcom,hw-settle-time = <2>;
		qcom,fast-avg-setup = <0>;
		qcom,vadc-thermal-node;
	};

	chan@13 {
		label = "msm_therm";
		reg = <0x13>;
		qcom,decimation = <0>;
		qcom,pre-div-channel-scaling = <0>;
		qcom,calibration-type = "ratiometric";
		qcom,scale-function = <2>;
		qcom,hw-settle-time = <2>;
		qcom,fast-avg-setup = <0>;
		qcom,vadc-thermal-node;
	};
};

&pm8953_mpps {
	msm_therm {
		msm_therm_default: msm_therm_default {
			pins = "mpp4";
			function = "analog";
			input-enable;
			qcom,amux-route = <3>;
		};
	};
	chg_therm {
		chg_therm_default: chg_therm_default {
			pins = "mpp2";
			function = "analog";
			input-enable;
			qcom,amux-route = <1>;
		};
	};
};

&thermal_zones {
	/delete-node/ case-therm-step;
	msm_therm {
		polling-delay-passive = <1000>;
		polling-delay = <1000>;
		thermal-sensors = <&pm8953_vadc 0x13>;
		thermal-governor = "user_space";
		thermal-user-alarm = <40000>;

		trips {
			active-config0 {
				temperature = <65000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};
	chg_therm {
		polling-delay-passive = <500>;
		polling-delay = <500>;
		thermal-sensors = <&pm8953_vadc 0x11>;
		thermal-governor = "user_space";
		thermal-user-alarm = <40000>;

		trips {
			active-config0 {
				temperature = <65000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};
	quiet_therm {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-sensors = <&pmi632_adc_tm 0x53>;
		thermal-governor = "user_space";

		trips {
			active-config0 {
				temperature = <65000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};
};

&pmi632_low_soc {
	temperature = <5>;
};

&bcl_soc {
	qcom,bcl-not-mitigate-cpu;
};
