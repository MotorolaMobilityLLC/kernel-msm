&qupv3_se7_i2c {
        status = "ok";
        aw882xx:aw882xx_smartpa@34 {
                compatible = "awinic,aw882xx_smartpa";
                reg = <0x34>;
                reset-gpio = <&tlmm 43 0>;
                irq-gpio = <&tlmm 91 0x2008>;
                monitor-flag = <1>;
                monitor-timer-val = <3000>;
                status = "okay";
        };
};
