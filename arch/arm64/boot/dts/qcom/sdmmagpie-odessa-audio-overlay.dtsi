#include <dt-bindings/interrupt-controller/irq.h>

&tlmm {
	ter_i2s_sck_ws {
		ter_i2s_sck_sleep: ter_i2s_sck_sleep {
			mux {
				pins = "gpio53", "gpio54";
				function = "ter_mi2s";
			};

			config {
				pins = "gpio53", "gpio54";
				drive-strength = <2>;   /* 2 mA */
				bias-disable;
			};
                };

                ter_i2s_sck_active: ter_i2s_sck_active {
			mux {
				pins = "gpio53", "gpio54";
				function = "ter_mi2s";
			};

			config {
				pins = "gpio53", "gpio54";
				drive-strength = <8>;   /* 8 mA */
				bias-disable;
			};
		};
        };

	ter_i2s_data0 {
		ter_i2s_data0_sleep: ter_i2s_data0_sleep {
			mux {
				pins = "gpio55";
				function = "ter_mi2s";
			};

			config {
				pins = "gpio55";
				drive-strength = <2>;   /* 2 mA */
				bias-disable;
			};
		};

                ter_i2s_data0_active: ter_i2s_data0_active {
			mux {
				pins = "gpio55";
				function = "ter_mi2s";
			};

			config {
				pins = "gpio55";
				drive-strength = <8>;   /* 8 mA */
				bias-disable;
			};
		};
        };

        ter_i2s_data1 {
                ter_i2s_data1_sleep: ter_i2s_data1_sleep {
			mux {
				pins = "gpio56";
				function = "ter_mi2s";
			};

			config {
				pins = "gpio56";
				drive-strength = <2>;   /* 2 mA */
				bias-disable;
			};
		};

		ter_i2s_data1_active: ter_i2s_data1_active {
			mux {
				pins = "gpio56";
				function = "ter_mi2s";
			};

			config {
				pins = "gpio56";
				drive-strength = <8>;   /* 8 mA */
				bias-disable;
			};
                };
        };
};

&soc {
	cdc_ter_mi2s_gpios: msm_cdc_pinctrl_ter {
		compatible = "qcom,msm-cdc-pinctrl";
                pinctrl-names = "aud_active", "aud_sleep";
                pinctrl-0 = <&ter_i2s_sck_active &ter_i2s_data0_active
			     &ter_i2s_data1_active>;
                pinctrl-1 = <&ter_i2s_sck_sleep &ter_i2s_data0_sleep
			      &ter_i2s_data1_sleep>;

	};

        msm_dai_mi2s: qcom,msm-dai-mi2s {
                compatible = "qcom,msm-dai-mi2s";

                dai_mi2s0: qcom,msm-dai-q6-mi2s-prim {
                        status = "okay";
                };

                dai_mi2s1: qcom,msm-dai-q6-mi2s-sec {
                        status = "okay";
                };

                dai_mi2s2: qcom,msm-dai-q6-mi2s-tert {
                        status = "okay";
                        compatible = "qcom,msm-dai-q6-mi2s";
                        qcom,msm-dai-q6-mi2s-dev-id = <2>;
                        qcom,msm-mi2s-rx-lines = <1>;
                        qcom,msm-mi2s-tx-lines = <2>;
                };

                dai_mi2s3: qcom,msm-dai-q6-mi2s-quat {
                        status = "okay";
                };

                dai_mi2s4: qcom,msm-dai-q6-mi2s-quin {
                        status = "okay";
                };

                dai_mi2s5: qcom,msm-dai-q6-mi2s-senary {
                        status = "okay";
                };
        };
};


