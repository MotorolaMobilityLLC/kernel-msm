&i2c_freq_custom {
	qcom,hw-thigh = <23>;
	qcom,hw-tlow = <30>;
	qcom,hw-tsu-sto = <20>;
	qcom,hw-tsu-sta = <20>;
	qcom,hw-thd-dat = <18>;
	qcom,hw-thd-sta = <19>;
	qcom,hw-tbuf = <31>;
	qcom,hw-scl-stretch-en = <0>;
	qcom,hw-trdhld = <3>;
};

&soc {
	qcom,csiphy@1628000 {
		qcom,clock-rates = <0 0 311000000 0 0 311000000 0
			0 311000000 0 0>;
	};

	qcom,csiphy@1629000 {
		qcom,clock-rates = <0 0 311000000 0 0 311000000 0
			0 311000000 0 0>;
	};

	qcom,csiphy@162a000 {
		qcom,clock-rates = <0 0 311000000 0 0 311000000 0
			0 311000000 0 0>;

	};

	vfe0: qcom,vfe0@5c10000 {
		qcom,clock-rates = <0 0 403200000 0 0 0 0 0 0 0
					0 0 576000000 0 0 0 0 0 0 0
					0 0 576000000 0 0 0 0 0 0 0>;
	};

	vfe1: qcom,vfe1@5c14000 {
		qcom,clock-rates = <0 0 403200000 0 0 0 0 0 0 0
					0 0 576000000 0 0 0 0 0 0 0
					0 0 576000000 0 0 0 0 0 0 0>;
	};

	led_flash_rear: qcom,camera-flash@0 {
		cell-index = <0>;
		compatible = "qcom,camera-flash";
		qcom,flash-source = <&pmi632_flash0>;
		qcom,torch-source = <&pmi632_torch0>;
		qcom,switch-source = <&pmi632_switch0>;
		status = "ok";
	};
};

&tlmm {
	cam_sensor_rear_gpio_active: cam_sensor_rear_gpio_active {
		/* MIPI_SEL */
		mux {
			pins = "gpio27";
			function = "gpio";
		};

		config {
			pins = "gpio27";
			bias-disable; /* No PULL */
			drive-strength = <2>; /* 2 MA */
		};
	};

	cam_sensor_rear_gpio_suspend: cam_sensor_rear_gpio_suspend {
		/* MIPI_SEL */
		mux {
			pins = "gpio27";
			function = "gpio";
		};

		config {
			pins = "gpio27";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
		};
	};

	cam_sensor_front_gpio_active: cam_sensor_front_gpio_active {
		/* MIPI_SEL */
		mux {
			pins = "gpio27";
			function = "gpio";
		};

		config {
			pins = "gpio27";
			bias-disable; /* No PULL */
			drive-strength = <2>; /* 2 MA */
		};
	};

	cam_sensor_front_gpio_suspend: cam_sensor_front_gpio_suspend {
		/* MIPI_SEL */
		mux {
			pins = "gpio27";
			function = "gpio";
		};

		config {
			pins = "gpio27";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
		};
	};

	cam_sensor_mclk3_active: cam_sensor_mclk3_active {
		/* MCLK3 */
		mux {
			pins = "gpio44";
			function = "cam_mclk";
		};

		config {
			pins = "gpio44";
			bias-disable; /* No PULL */
			drive-strength = <2>; /* 2 MA */
		};
	};

	cam_sensor_mclk3_suspend: cam_sensor_mclk3_suspend {
		/* MCLK3 */
		mux {
			pins = "gpio44";
			function = "cam_mclk";
		};

		config {
			pins = "gpio44";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
		};
	};
	cam_sensor_video_gpio_active: cam_sensor_video_gpio_active {
		/* RESET */
		mux {
			pins = "gpio41";
			function = "gpio";
		};

		config {
			pins = "gpio41";
			bias-disable; /* No PULL */
			drive-strength = <2>; /* 2 MA */
		};
	};

	cam_sensor_video_gpio_suspend: cam_sensor_video_gpio_suspend {
		/* RESET */
		mux {
			pins = "gpio41";
			function = "gpio";
		};

		config {
			pins = "gpio41";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
		};
	};
};

&cci {
	actuator_rear: qcom,actuator@0 {
		cell-index = <0>;
		reg = <0x0>;
		compatible = "qcom,actuator";
		qcom,cci-master = <0>;
	};

	actuator_macro: qcom,actuator@1 {
		cell-index = <1>;
		reg = <0x1>;
		compatible = "qcom,actuator";
		qcom,cci-master = <1>;
	};

	eeprom_rear: qcom,eeprom@0 {
		cell-index = <0>;
		compatible = "qcom,eeprom";
		reg = <0x0>;
		cam_vio-supply = <&L12A>;
		cam_clk-supply = <&camss_top_gdsc>;
		qcom,cam-vreg-name = "cam_vio";
		qcom,cam-vreg-min-voltage = <1800000>;
		qcom,cam-vreg-max-voltage = <1800000>;
		qcom,cam-vreg-op-mode = <80000>;
		qcom,gpio-no-mux = <0>;
		pinctrl-names = "cam_default", "cam_suspend";
		pinctrl-0 = <&cam_sensor_mclk0_active
			&cam_sensor_rear_active
			&cam_sensor_rear_gpio_active
			&cam_vaf_en_default
			&cam_vdig_en_default
			&cam_vana_en_default>;
		pinctrl-1 = <&cam_sensor_mclk0_suspend
			&cam_sensor_rear_suspend
			&cam_sensor_rear_gpio_suspend
			&cam_vaf_en_default
			&cam_vdig_en_default
			&cam_vana_en_default>;
		gpios = <&tlmm 34 0>,
			<&tlmm 48 0>,
			<&pmi632_gpios 2 0>,
			<&pmi632_gpios 8 0>,
			<&pmi632_gpios 5 0>,
			<&tlmm 27 0>;
		qcom,gpio-reset = <1>;
		qcom,gpio-vaf = <2>;
		qcom,gpio-vana = <3>;
		qcom,gpio-vdig = <4>;
		qcom,gpio-custom1 = <5>;
		qcom,gpio-req-tbl-num = <0 1 2 3 4 5>;
		qcom,gpio-req-tbl-flags = <1 0 0 0 0 0>;
		qcom,gpio-req-tbl-label = "CAM_R_MCLK",
					"CAM_R_RESET",
					"CAM_R_AF",
					"CAM_R_AVDD",
					"CAM_R_DVDD",
					"CAM_MIPI_SEL";
		qcom,cci-master = <0>;
		status = "ok";
		clocks = <&clock_gcc GCC_CAMSS_MCLK0_CLK_SRC>,
			<&clock_gcc GCC_CAMSS_MCLK0_CLK>;
		clock-names = "cam_src_clk", "cam_clk";
		qcom,clock-rates = <24000000 0>;
	};

