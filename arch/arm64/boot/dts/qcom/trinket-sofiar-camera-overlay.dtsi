&pmi632_gpios {
	cam_vaf {
		cam_vaf_en_default: cam_vaf_en_default {
			pins = "gpio2";
			function = "func1";
			output-enable;
			drive-push-pull;
		};
	};

	cam_vdig {
		cam_vdig_en_default: cam_vdig_en_default {
			pins = "gpio5";
			function = "func1";
			output-enable;
			drive-push-pull;
		};
	};

	cam_vana {
		cam_vana_en_default: cam_vana_en_default {
			pins = "gpio8";
			function = "func1";
			output-enable;
