&pmi632_gpios {
	rear_camera_dvdd_en {
		rear_camera_dvdd_en_active: rear_camera_dvdd_en_active {
			pins = "gpio5";
			function = "normal";
			output-enable;
			bias-pull-down;
			output-high;
			drive-push-pull;
			};
		rear_camera_dvdd_en_suspend: rear_camera_dvdd_en_suspend {
			pins = "gpio5";
			function = "normal";
			output-enable;
			bias-pull-down;
			output-low;
			drive-push-pull;
			};
		};
	rear_camera_avdd_en {
		rear_camera_avdd_en_active: rear_camera_avdd_en_active {
			pins = "gpio8";
			function = "normal";
			output-enable;
			bias-pull-down;
			output-high;
			drive-push-pull;
		};
		rear_camera_avdd_en_suspend: rear_camera_avdd_en_suspend {
			pins = "gpio8";
			function = "normal";
			output-enable;
			bias-pull-down;
			//output-high;
			output-low;
			drive-push-pull;
		};
	      };

};

&cci {
	actuator0: qcom,actuator@0 {
		cell-index = <0>;
		reg = <0x0>;
		compatible = "qcom,actuator";
		qcom,cci-master = <0>;
		cam_vaf-supply = <&cam_vaf_gpio_regulator>;
		qcom,cam-vreg-name = "cam_vaf";
		qcom,cam-vreg-min-voltage = <2800000>;
		qcom,cam-vreg-max-voltage = <2800000>;
		qcom,cam-vreg-op-mode = <0>;
	};

	actuator1: qcom,actuator@1 {
		cell-index = <1>;
		reg = <0x1>;
		compatible = "qcom,actuator";
		qcom,cci-master = <1>;
		cam_vaf-supply = <&cam_vaf_gpio_regulator>;
		qcom,cam-vreg-name = "cam_vaf";
		qcom,cam-vreg-min-voltage = <2800000>;
		qcom,cam-vreg-max-voltage = <2800000>;
		qcom,cam-vreg-op-mode = <0>;
	};

	eeprom0: qcom,eeprom@0 {
		cell-index = <0>;
		reg = <0>;
		compatible = "qcom,eeprom";
		cam_vio-supply = <&L12A>;
		cam_vaf-supply = <&cam_vaf_gpio_regulator>;
		cam_clk-supply = <&camss_top_gdsc>;
		qcom,cam-vreg-name = "cam_vio","cam_vaf","cam_clk";
		qcom,cam-vreg-min-voltage = <1800000 2800000 0>;
		qcom,cam-vreg-max-voltage = <1800000 2800000 0>;
		qcom,cam-vreg-op-mode = <105000 100000 0>;
		qcom,gpio-no-mux = <0>;
		pinctrl-names = "cam_default", "cam_suspend";
		pinctrl-0 = <&cam_sensor_mclk0_active
				&rear_cam_sensor_active
				&rear_camera_dvdd_en_active
				&rear_camera_avdd_en_active>;
		pinctrl-1 = <&cam_sensor_mclk0_suspend
				&rear_cam_sensor_suspend
				&rear_camera_dvdd_en_suspend
				&rear_camera_avdd_en_suspend>;
		gpios = <&tlmm 34 0>,
			<&tlmm 48 0>,
			<&pmi632_gpios 8 0>,
			<&pmi632_gpios 5 0>;
		qcom,gpio-reset = <1>;
		qcom,gpio-vana = <2>;
		qcom,gpio-vdig = <3>;
		qcom,gpio-req-tbl-num = <0 1 2 3 >;
		qcom,gpio-req-tbl-flags = <1 0 0 0 >;
		qcom,gpio-req-tbl-label = "REAR_CAMIF_MCLK0",
					"REAR_CAM_RESET",
					"REAR_CAM_AVDD",
					"REAR_CAM_DVDD";
		qcom,sensor-position = <0>;
		qcom,sensor-mode = <0>;
		qcom,cci-master = <0>;
		status = "ok";
		clocks = <&clock_gcc GCC_CAMSS_MCLK0_CLK_SRC>,
				<&clock_gcc GCC_CAMSS_MCLK0_CLK>;
		clock-names = "cam_src_clk", "cam_clk";
		qcom,clock-rates = <24000000 0>;
	};

	eeprom1: qcom,eeprom@1 {
		cell-index = <1>;
		reg = <1>;
		compatible = "qcom,eeprom";
		cam_vio-supply = <&L12A>;
		cam_clk-supply = <&camss_top_gdsc>;
		qcom,cam-vreg-name = "cam_vio";
		qcom,cam-vreg-min-voltage = <1800000>;
		qcom,cam-vreg-max-voltage = <1800000>;
		qcom,cam-vreg-op-mode = <105000>;
		qcom,gpio-no-mux = <0>;
		pinctrl-names = "cam_default", "cam_suspend";
		pinctrl-0 = <&cam_sensor_mclk1_active
				 &front_cam_sensor_active
                                      &rear_camera_avdd_en_active>;
		pinctrl-1 = <&cam_sensor_mclk1_suspend
				 &front_cam_sensor_suspend
                                      &rear_camera_avdd_en_suspend>;
		gpios = <&tlmm 35 0>,
			<&tlmm 42 0>,
                        <&pmi632_gpios 8  0>,
                        <&tlmm 84 0>;
		qcom,gpio-reset = <1>;
                qcom,gpio-vana = <2>;
                qcom,gpio-vdig = <3>;
		qcom,gpio-req-tbl-num = <0 1 2 3 >;
		qcom,gpio-req-tbl-flags = <1 0 0 0>;
		qcom,gpio-req-tbl-label = "FRONT_CAMIF_MCLK1",
					   "FRONT_CAM_RESET1",
                                           "FRONT_CAM_VANA1",
                                           "FRONT_CAM_VDIG1";
		qcom,sensor-position = <0>;
		qcom,sensor-mode = <0>;
		qcom,cci-master = <0>;
		status = "ok";
		clocks = <&clock_gcc GCC_CAMSS_MCLK1_CLK_SRC>,
			<&clock_gcc GCC_CAMSS_MCLK1_CLK>;
		clock-names = "cam_src_clk", "cam_clk";
		qcom,clock-rates = <24000000 0>;
	};

	eeprom2: qcom,eeprom@2 {
