&pmi632_flash0 {
			label = "flash";
			qcom,led-name = "led:flash_0";
			qcom,max-current = <1200>;
			qcom,default-led-trigger = "flash0_trigger";
			qcom,id = <0>;
			qcom,current-ma = <1000>;
			qcom,duration-ms = <1280>;
			qcom,ires-ua = <12500>;
			qcom,hdrm-voltage-mv = <400>;
			qcom,hdrm-vol-hi-lo-win-mv = <100>;
};
&tlmm {
		rear_cam_sensor_active: rear_cam_sensor_active {
			/* REAR CAMERA */
			mux {
				pins = "gpio48";
				function = "gpio";
			};

			config {
				pins = "gpio48";
				bias-disable; /* No PULL */
				drive-strength = <2>; /* 2 MA */
			};
		};

		rear_cam_sensor_suspend: rear_cam_sensor_suspend {
			/* REAR CAMERA */
			mux {
				pins = "gpio48";
				function = "gpio";
			};

			config {
				pins = "gpio48";
				bias-pull-down; /* PULL DOWN */
				drive-strength = <2>; /* 2 MA */
				output-low;
			};
		};

		front_cam_sensor_active: front_cam_sensor_active {
			/* FRONT CAMERA */
			mux {
				pins = "gpio42","gpio84";
				function = "gpio";
			};

			config {
				pins = "gpio42","gpio84";
				bias-disable; /* No PULL */
				drive-strength = <2>; /* 2 MA */
			};
		};

		front_cam_sensor_suspend: front_cam_sensor_suspend {
			/* FRONT CAMERA */
			mux {
				pins = "gpio42","gpio84";
				function = "gpio";
			};

			config {
				pins = "gpio42","gpio84";
				bias-pull-down; /* PULL DOWN */
				drive-strength = <2>; /* 2 MA */
				output-low;
			};
		};


		wide_cam_sensor_active: wide_cam_sensor_active {
			/* WIDE CAMERA */
			mux {
				pins = "gpio46","gpio26","gpio84","gpio2";
				function = "gpio";
			};

			config {
				pins = "gpio46","gpio26","gpio84","gpio2";
				bias-disable; /* No PULL */
				drive-strength = <2>; /* 2 MA */
			};
		};

		wide_cam_sensor_suspend: wide_cam_sensor_suspend {
			/* WIDE CAMERA */
			mux {
				pins = "gpio46","gpio26","gpio84","gpio2";
				function = "gpio";
			};

			config {
				pins = "gpio46","gpio26","gpio84","gpio2";
				bias-pull-down; /* PULL DOWN */
				drive-strength = <2>; /* 2 MA */
				output-low;
			};
		};

		micro_cam_sensor_active: micro_cam_sensor_active {
			mux {
				pins = "gpio2","gpio26","gpio84";
				function = "gpio";
			};

			config {
				pins = "gpio2","gpio26","gpio84";
				bias-disable;
				drive-strength = <2>;
			};
		};

		micro_cam_sensor_suspend: micro_cam_sensor_suspend {
			mux {
				pins = "gpio2","gpio26","gpio84";
				function = "gpio";
			};

			config {
				pins = "gpio2","gpio26","gpio84";
				bias-pull-down;
				drive-strength = <2>;
				output-low;
			};
		};
		cam_sensor_mclk3_active: cam_sensor_mclk3_active {
			mux {
				pins = "gpio44";
				function = "cam_mclk";
			};

			config {
				pins = "gpio44";
				bias-disable;
				drive-strength = <2>;
			};
		};

		cam_sensor_mclk3_suspend: cam_sensor_mclk3_suspend {
			mux {
				pins = "gpio44";
				function = "cam_mclk";
			};

			config {
				pins = "gpio44";
				bias-pull-down;
				drive-strength = <2>;
			};
		};
	};
&soc {

	qcom,csiphy@1628000 {
		qcom,clock-rates = <0 0 311000000 0 0 311000000 0
			0 311000000 0 0>;
	};

	qcom,csiphy@1629000 {
		qcom,clock-rates = <0 0 311000000 0 0 311000000 0
			0 311000000 0 0>;
	};

	qcom,csiphy@162a000 {
		qcom,clock-rates = <0 0 311000000 0 0 311000000 0
			0 311000000 0 0>;

	};

	vfe0: qcom,vfe0@5c10000 {
		qcom,clock-rates = <0 0 403200000 0 0 0 0 0 0 0
					0 0 576000000 0 0 0 0 0 0 0
					0 0 576000000 0 0 0 0 0 0 0>;
	};

	vfe1: qcom,vfe1@5c14000 {
		qcom,clock-rates = <0 0 403200000 0 0 0 0 0 0 0
					0 0 576000000 0 0 0 0 0 0 0
					0 0 576000000 0 0 0 0 0 0 0>;
	};

	led_flash_rear: qcom,camera-flash@0 {
		cell-index = <0>;
		compatible = "qcom,camera-flash";
		qcom,flash-source = <&pmi632_flash0>;
		qcom,torch-source = <&pmi632_torch0>;
		qcom,switch-source = <&pmi632_switch0>;
		status = "ok";
	};
	cam_vaf_gpio_regulator: cam_vaf_gpio_regulator {
		compatible = "regulator-fixed";
		regulator-name = "cam_vaf_gpio_regulator";
		regulator-min-microvolt = <2800000>;
		regulator-max-microvolt = <2800000>;
		gpio = <&tlmm 47 0>;
		enable-active-high;
		startup-delay-us = <300>;
	};

};
&pm6125_gpios {
	micro_cam_vana {
		micro_cam_vana_default: micro_cam_vana_default{
			pins = "gpio2";
			function = "func1";
			qcom,drive-strength = <2>;
			power-source = <0>;
			bias-disable;
			output-low;
		};
	};
