			};
		};
	};

	haptic_reset {
		haptic_rst_default: haptic_rst_default {
			mux {
				pins = "gpio80";
				function = "gpio";
			};

			config {
				pins = "gpio80";
				drive-strength = <2>; /* 2 mA */
