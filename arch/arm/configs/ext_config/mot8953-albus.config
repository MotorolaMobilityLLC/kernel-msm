# albus configs
CONFIG_ALBUS_DTB=y
CONFIG_HAPTIC_DRV2624=y
# CONFIG_STMVL53L0 is not set
CONFIG_STMVL53L1=y
# CONFIG_SND_SOC_CS35L34 is not set
CONFIG_SND_SOC_CS35L35=y
# CONFIG_SENSORS_MOTOSH is not set
# CONFIG_SENSORS_MOTOSH_HEADSET is not set
# CONFIG_SENSORS_FPC_1020 is not set
CONFIG_SENSORS_FPC_PLATFORM_1020=y

# CONFIG_TOUCHSCREEN_HIMAX_* is new in P, disable on albus for P bringup
CONFIG_TOUCHSCREEN_HIMAX_CHIPSET=n
CONFIG_TOUCHSCREEN_HIMAX_I2C=n
CONFIG_TOUCHSCREEN_HIMAX_DEBUG=n
CONFIG_USB_NET_RNDIS_HOST=y
CONFIG_DWC3_HOST_USB3_LPM_ENABLE=y
