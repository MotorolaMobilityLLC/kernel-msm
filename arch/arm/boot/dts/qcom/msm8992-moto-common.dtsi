		CPU1: cpu@1 {
			current = < 17347 //384000 kHz
				24959 //460800 kHz
				30570 //600000 kHz
				39367 //672000 kHz
				44313 //787200 kHz
				54043 //864000 kHz
				66856 //960000 kHz
				105574 //1248000 kHz
				139040>; //1440000 kHz
		};
		CPU2: cpu@2 {
			current = < 17347 //384000 kHz
