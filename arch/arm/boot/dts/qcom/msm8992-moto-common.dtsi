		qcom,vin-sel = <2>;      /* VIN2 */
		qcom,src-sel = <0>;      /* GPIO */
		qcom,master-en = <1>;    /* Enable GPIO */
	};

	gpio@cc00 { /* GPIO 13 - NFC_ENABLE_N */
		status = "okay";
