/dts-v1/;

#include "msm8916-styx.dtsi"

/ {
	qcom,board-id = <0x43 0x81bd>,
			<0x43 0x82ad>,
			<0x45 0x81b1>;
};

&i2c_1 {
	fsa8500@23 {
		status = "ok";
	};
};

&spi_0 {
	stml0xx@0 {
		headset_detect_enable = <0>;
	};
};

&spi_5 {
	status = "ok";
	nmi326@0 {
		status = "ok";
	};
};
