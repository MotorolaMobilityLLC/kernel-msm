&tlmm {
	usb_id_int_active {
		usb_id_active: default {
			mux {
				pins = "gpio59";
				function = "gpio";
			};
			config {
				pins = "gpio59";
				drive-strength = <8>;
				bias-pull-up;
			};
		};
	};
	usb_id_int_sleep {
		usb_id_sleep: sleep {
			mux {
				pins = "gpio59";
				function = "gpio";
			};
			config {
				pins = "gpio59";
				drive-strength = <8>;
				bias-disable;
			};
		};
	};
};
