/dts-v1/;

#include "msm8916-styx.dtsi"

/ {
	qcom,board-id = <0x43 0x82a0>;
};

&i2c_1 {
	fsa8500@23 {
		status = "ok";
	};
};

&spi_0 {
	stml0xx@0 {
		headset_detect_enable = <0>;
	};
};

&pm8916_mpps {
	mpp@a100 { /* MPP 2 */
		/* CCP_RESOUT_N */
		status = "ok";
		qcom,mode = <0>;      /* Digital Input */
		qcom,vin-sel = <2>;   /* VIN 2 (1.8V) */
		qcom,src-sel = <0>;   /* GPIO */
		qcom,master-en = <1>; /* Enable MPP */
	};
};
