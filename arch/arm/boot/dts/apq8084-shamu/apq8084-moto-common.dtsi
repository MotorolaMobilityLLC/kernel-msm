&pma8084_gpios {
	gpio@c000 { /* GPIO 1 */
		/* Wireless Charger Pad Detect N */
		qcom,mode = <0>;	/* Digital input */
		qcom,pull = <5>;	/* No pull */
		qcom,vin-sel = <2>;      /* VIN2 */
                qcom,src-sel = <0>;      /* GPIO */
                qcom,master-en = <1>;    /* Enable GPIO */
	};

	gpio@c100 { /* GPIO 2 */
		qcom,mode = <0>;
		qcom,pull = <0>;
		qcom,vin-sel = <2>;
		qcom,src-sel = <0>;
	};

	gpio@c200 { /* GPIO 3 */
	};

	gpio@c300 { /* GPIO 4 */
		qcom,mode = <1>;         /* Digital output*/
		qcom,pull = <4>;         /* Pulldown 10uA */
		qcom,vin-sel = <2>;      /* VIN2 */
		qcom,src-sel = <0>;      /* GPIO */
		qcom,invert = <0>;       /* Invert */
		qcom,master-en = <1>;    /* Enable GPIO */
	};

	gpio@c400 { /* GPIO 5 */
		/* Buttons Backlight PWM */
		qcom,mode = <1>; 		/* Digital output */
		qcom,vin-sel = <2>;		/* PMA8084 S4 = 1.8 V */
		qcom,src-sel = <2>;		/* FUNC1 = LPG2 output */
		qcom,output-type = <0>;		/* CMOS logic */
	};

	gpio@c500 { /* GPIO 6 */
		/* Wireless Charging - CHG_GOOD_N */
		qcom,mode = <0>;        /* QPNP_PIN_MODE_DIG_IN */
		qcom,pull = <5>;        /* QPNP_PIN_PULL_NO */
		qcom,vin-sel = <2>;     /* QPNP_PIN_VIN2 (S4 - 1.8V) */
		qcom,src-sel = <0>;     /* QPNP_PIN_SEL_FUNC_CONSTANT */
		qcom,master-en = <1>;
	};

	gpio@c600 { /* GPIO 7 */
		qcom,master-en = <0>;    /* Disable GPIO */
	};

	gpio@c700 { /* GPIO 8 */
		qcom,master-en = <0>;    /* Disable GPIO */
	};

	gpio@c800 { /* GPIO 9 */
		qcom,master-en = <0>;    /* Disable GPIO */
	};

	gpio@c900 { /* GPIO 10 */
		qcom,mode = <0>;		/* QPNP_PIN_MODE_DIG_IN */
		qcom,pull = <4>;		/* QPNP_PIN_PULL_DN */
		qcom,src-sel = <2>;		/* QPNP_PIN_SEL_FUNC_1 */
		qcom,vin-sel = <2>;		/* QPNP_PIN_VIN2 */
		qcom,master-en = <1>;
	};

	gpio@ca00 { /* GPIO 11 */
		/* VBUS interrupt */
		qcom,mode = <0>;
		qcom,pull = <4>;		/* QPNP_PIN_PULL_DN */
		qcom,vin-sel = <2>;
		qcom,src-sel = <0>;
	};

	gpio@cb00 { /* GPIO 12 */
	};

	gpio@cc00 { /* GPIO 13 */
		/* USB_VBUS_DET */
		qcom,mode = <0>;               /* QPNP_PIN_MODE_DIG_IN */
		qcom,pull = <5>;               /* NO PULL */
		qcom,vin-sel = <2>;            /* QPNP_PIN_VIN2 */
		qcom,src-sel = <0>;            /* GPIO */
		qcom,master-en = <1>;          /* Enable GPIO */
	};

	gpio@cd00 { /* GPIO 14 */
		/* SMB349 STAT */
		qcom,mode = <0>;
		qcom,pull = <5>;		/* N0 PULL*/
		qcom,vin-sel = <2>;
		qcom,src-sel = <0>;
	};

	gpio@ce00 { /* GPIO 15 */
		qcom,mode = <1>;		/* Digital output */
		qcom,output-type = <0>;		/* CMOS logic */
		qcom,invert = <0>;		/* Output low initially */
		qcom,vin-sel = <2>;		/* VIN 2 */
		qcom,src-sel = <2>;		/* Function 1 */
		qcom,out-strength = <2>;	/* Medium */
		qcom,master-en = <1>;		/* Enable GPIO */
	};

	gpio@cf00 { /* GPIO 16 */
		qcom,mode = <1>;		/* Digital output */
		qcom,output-type = <0>;		/* CMOS logic */
		qcom,pull = <5>;		/* No pull */
		qcom,vin-sel = <2>;		/* VIN 2 */
		qcom,out-strength = <3>;	/* High */
		qcom,src-sel = <3>;       	/* QPNP_PIN_SEL_FUNC_2  */
		qcom,master-en = <1>;		/* Enable GPIO */
	};

	gpio@d000 { /* GPIO 17 */
		/* Wireless Charge Complete N */
		qcom,mode = <1>;		/* Digital output */
		qcom,output-type = <0>;		/* CMOS */
		qcom,pull = <5>;		/* No pull */
		qcom,vin-sel = <2>;		/* VIN 2 */
		qcom,src-sel = <0>;		/* Constant */
		qcom,out-strength = <1>;	/* Low */
		qcom,master-en = <1>;		/* Enable GPIO */
	};

	gpio@d100 { /* GPIO 18 */
		qcom,mode = <1>;		/* Digital output */
		qcom,output-type = <0>;		/* CMOS logic */
		qcom,invert = <0>;		/* Output low initially */
		qcom,vin-sel = <2>;		/* VIN 2 */
		qcom,src-sel = <3>;		/* Function 2 */
		qcom,out-strength = <2>;	/* Medium */
		qcom,master-en = <1>;		/* Enable GPIO */
	};

	gpio@d200 { /* GPIO 19 */
	};

	gpio@d300 { /* GPIO 20 */
		/* WLAN_3p3v_vreg regulator enable */
		qcom,mode = <1>;		/* Digital output */
		qcom,output-type = <0>;		/* CMOS logic */
		qcom,invert = <0>;		/* Output low initially */
		qcom,vin-sel = <0>;		/* VPH_PWR */
		qcom,src-sel = <0>;		/* Constant */
		qcom,out-strength = <1>;	/* Low */
		qcom,master-en = <1>;		/* Enable GPIO */
	};

	gpio@d400 { /* GPIO 21 */
		qcom,mode = <0>;
		qcom,pull = <4>;		/* QPNP_PIN_PULL_DN */
		qcom,vin-sel = <2>;
		qcom,src-sel = <0>;
	};

	gpio@d500 { /* GPIO 22 */
	};
};

&pma8084_mpps {
	mpp@a000 { /* MPP 1 */
	};

	mpp@a100 { /* MPP 2 */
		/* USB_ID */
		qcom,mode = <0>;          /* QPNP_PIN_MODE_DIG_IN */
		qcom,pull = <1>;          /* QPNP_PIN_MPP_PULL_UP_OPEN */
		qcom,vin-sel = <3>;	  /* LDO6 1.8V */
		qcom,src-sel = <0>;	  /* Constant */
		qcom,ain-route = <1>;     /* QPNP_PIN_AIN_AMUX_CH6 */
		qcom,master-en = <1>;     /* Enable */

	};

	mpp@a200 { /* MPP 3 */
		/* HDMI_5v_vreg regulator enable */
		qcom,mode = <1>;		/* Digital output */
		qcom,output-type = <0>;		/* CMOS logic */
		qcom,vin-sel = <0>;		/* VPH_PWR */
		qcom,src-sel = <0>;		/* Constant */
		qcom,master-en = <1>;		/* Enable GPIO */
	};

	mpp@a300 { /* MPP 4 */
		qcom,mode = <4>;          /* QPNP_PIN_MODE_AIN */
		qcom,ain-route = <3>;	/* QPNP_PIN_AIN_AMUX_CH8 */
		qcom,master-en = <1>;		/* Enable */
	};

	mpp@a400 { /* MPP 5 */
		/* BCM NFC WAKE */
		qcom,mode = <1>;		/* Digital output */
		qcom,output-type = <0>;		/* CMOS logic */
		qcom,vin-sel = <2>;		/* PMA8084 S4 = 1.8V > 1.6V */
		qcom,src-sel = <0>;		/* Constant */
		qcom,master-en = <1>;		/* Enable GPIO */
	};

	mpp@a500 { /* MPP 6 */
		qcom,mode = <0>;          /* QPNP_PIN_MODE_DIG_IN */
		qcom,pull = <1>;	  /* No pull */
		qcom,vin-sel = <2>;
		qcom,master-en = <1>;
	};

	mpp@a600 { /* MPP 7 */
		qcom,mode = <4>;          /* QPNP_PIN_MODE_AIN */
		qcom,ain-route = <2>;	/* QPNP_PIN_AIN_AMUX_CH7 */
		qcom,master-en = <1>;
	};

	mpp@a700 { /* MPP 8 */		/* Sign Of Life LED Sink*/
		qcom,mode = <6>;	/* QPNP_PIN_MODE_SINK */
	};
};

