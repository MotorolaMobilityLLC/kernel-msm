		maximum-speed = "high-speed";
	};
};

&hsphy0 {
	qcom,hsphy-init = <0x00D7D9A4>;
};

&hsphy1 {
	qcom,hsphy-init = <0x00D7D9A4>;
};

&soc {
	/delete-node/ qcom,pcie@fc528000;
	/delete-node/ qcom,mhi;
};

/* This is VREG_5P0, controlled via MPP3 */
&vbus_det_vreg {
	qcom,init-voltage = <5000000>;
};
