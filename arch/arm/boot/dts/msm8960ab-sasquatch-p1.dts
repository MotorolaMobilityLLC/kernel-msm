/dts-v1/;

/include/ "msm8960ab-sasquatch-common.dtsi"

/ {
	mmi,revision = "p1";

	qcom,msm8960-pinmux@0x801000 {
		compatible = "qcom,msm8960-pinmux";
		#address-cells = <1>;
		#size-cells = <1>;

		pin@801350 {  /* AP_TO_SP_FLASH_EN                          */
			reg = <0x801350 0x10>;
			qcom,pin-num = <53>;
			qcom,active@0 {
				qcom,func = <0>;  /* gpio_in_out(53)        */
				qcom,pull = <0>;  /* none                   */
				qcom,drv = <0>;   /* 2ma                    */
				qcom,dir = <2>;   /* out_low                */
			};
			qcom,suspended@1 {
				qcom,func = <0>;  /* gpio_in_out(53)        */
				qcom,pull = <0>;  /* none                   */
				qcom,drv = <0>;   /* 2ma                    */
				qcom,dir = <2>;   /* out_low                */
			};
		};
	};

	/* PM8921 - MPP */
	qcom,msm-pm8921-mpps {
		mux {
			mpp@9 {
				/* EMU_DMB_PPD_DET_N */
				qcom,pm8xxx-mpp-pin = <9>;

				/*
				 * type   : D_INPUT
				 * level  : L17
				 * ctrl   : DIN_TO_INT
				 */
				qcom,pm8xxx-mpp-type = <0>;
				qcom,pm8xxx-mpp-level = <4>;
				qcom,pm8xxx-mpp-ctrl = <0>;
			};
		};
	};

};

/* GSBI10 - I2C */
&i2c10 {
	/* TI tlv320aic3253 */
	aic3253@18 {
		compatible = "ti,aic3253";
		reg = <0x18>;
		gpios = <&pm8921_gpio 18 0>, /* RESET */
			<0>; 		     /* MCLK  */
	};
};
