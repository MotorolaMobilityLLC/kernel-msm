/include/ "../skeleton.dtsi"

/ {
	qcom,cci@fda0C000 {
		qcom,camera@20 {
			status = "ok";
			compatible = "qcom,imx179";
			reg = <0x20 0x0>;
			qcom,slave-id = <0x20 0x0002 0x8179>;
			qcom,csiphy-sd-index = <0>;
			qcom,csid-sd-index = <0>;
			qcom,actuator-src = <&actuator0>;
			qcom,flash-type = <0>;
			qcom,led-flash-src = <&led_flash0>;
			qcom,mount-angle = <90>;
			qcom,sensor-name = "imx179";
			cam_vdig-supply = <&pm8941_l3>;
			qcom,cam-vreg-name = "cam_vdig", "cam_vio", "cam_vana", "cam_vaf";
			qcom,cam-vreg-type = <0 0 0 0 >;
			qcom,cam-vreg-min-voltage = <1225000 0 0 0>;
			qcom,cam-vreg-max-voltage = <1225000 0 0 0>;
			qcom,cam-vreg-op-mode = <1225000 0 0 0>;
			qcom,gpio-no-mux = <0>;
			gpios = <&msmgpio 15 0>,  // MCLK
				<&msmgpio 4 0>,   // MAIN_CAM_RESET
				<&msmgpio 16 0>,  // 13M_VANA_EN
				<&msmgpio 96 0>,  // 13M_VIO_EN
				<&msmgpio 57 0>;  // 13M_VCM_EN
			qcom,gpio-reset = <1>;
			qcom,gpio-vana = <2>;
			qcom,gpio-vio = <3>;
			qcom,gpio-vcm = <4>;
			qcom,gpio-req-tbl-num = <0 1 2 3 4>;
			qcom,gpio-req-tbl-flags = <0 0 0 0 0>;
			qcom,gpio-req-tbl-label =  "CAMIF_MCLK", "CAM_RESET1", "MAIN_VANA_EN", "MAIN_VIO_EN", "MAIN_VCM_EN";
			qcom,csi-lane-assign = <0x4320>;
			qcom,csi-lane-mask = <0x1F>;
			qcom,sensor-position = <0>;
			qcom,sensor-mode = <0>;
			qcom,cci-master = <0>;
		};

	qcom,camera@90 {
			status = "ok";
			compatible = "qcom,mt9m114";
			reg = <0x90 0x0>;
			qcom,slave-id = <0x90 0x0 0x2481>;
			qcom,csiphy-sd-index = <2>;
			qcom,csid-sd-index = <0>;
			qcom,flash-type = <0>;
			qcom,mount-angle = <270>;
			qcom,sensor-name = "mt9m114";
			cam_vana-supply = <&pm8941_l18>;
			cam_vio-supply = <&pm8941_lvs2>;
			qcom,cam-vreg-name = "cam_vdig", "cam_vio", "cam_vana";
			qcom,cam-vreg-type = <0 1 0>;
			qcom,cam-vreg-min-voltage = <0 0 2850000>;
			qcom,cam-vreg-max-voltage = <0 0 2850000>;
			qcom,cam-vreg-op-mode = <0 0 2850000>;
			qcom,gpio-no-mux = <0>;
			gpios = <&msmgpio 17 0>, //VT_CAM_MCLK
				<&msmgpio 18 0>, //VT_CAM_RESET
				<&msmgpio 30 0>; //VT_LDO_EN
			qcom,gpio-reset = <1>;
			qcom,gpio-vdig = <2>;
			qcom,gpio-req-tbl-num = <0 1 2>;
			qcom,gpio-req-tbl-flags = <0 0 0>;
			qcom,gpio-req-tbl-label = "CAMIF_MCLK2",
						     "CAM_RESET2",
						     "VT_LDO_EN";
			qcom,csi-lane-assign = <0x4320>;
			qcom,csi-lane-mask = <0x3>;
			qcom,sensor-position = <1>;
			qcom,sensor-mode = <1>;
			qcom,cci-master = <1>;
		};
	};
};
