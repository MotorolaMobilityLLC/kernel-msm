&sdcc1 {
	qcom,clk-rates = <400000 20000000 25000000 50000000 100000000>;
	qcom,bus-speed-mode = "DDR_1p8v";
	qcom,msm-bus,num-cases = <7>;
	/* XXX: No 200 MB/s */
	qcom,msm-bus,vectors-KBps = <78 512 0 0>, /* No vote */
		<78 512 1600 3200>,    /* 400 KB/s*/
		<78 512 80000 160000>, /* 20 MB/s */
		<78 512 100000 200000>, /* 25 MB/s */
		<78 512 200000 400000>, /* 50 MB/s */
		<78 512 400000 800000>, /* 100 MB/s */
		<78 512 2048000 4096000>; /* Max. bandwidth */
	qcom,bus-bw-vectors-bps = <0 400000 20000000 25000000 50000000 100000000 4294967295>;
	status = "ok";
};

&sdcc2 {
	#address-cells = <0>;
	interrupt-parent = <&sdcc2>;
	interrupts = <0 1 2>;
	#interrupt-cells = <1>;
	interrupt-map-mask = <0xffffffff>;

	/* Qualcomm / Broadcom WiFi solution definition. You must choose one solution */

	/* Qualcomm WiFi solution - Start */
	/*
	interrupt-map = <0 &intc 0 125 0
		1 &intc 0 220 0
		2 &msmgpio 35 0x3>;
	interrupt-names = "core_irq", "bam_irq", "wlan_oob_irq";
	qcom,bus-width = <4>;

	vdd-supply = <&pm8941_l11>;
	qcom,sdcc-sup-voltages = <1800 2950>;
	qcom,sdcc-nonremovable = <1>;
	qcom,sup-voltages = <1800 2950>;
	qcom,nonremovable;
	*/
	/* Qualcomm WiFi solution - End */

	/* Broadcom WiFi solution - Start */
	interrupt-map = <0 &intc 0 125 0
		1 &intc 0 220 0
		2 &msmgpio 74 0x3>;
	interrupt-names = "core_irq", "bam_irq", "wlan_oob_irq";
	qcom,bus-width = <4>;

	qcom,sdcc-sup-voltages = <1800 2950>;
	qcom,sup-voltages = <1800 2950>;
	qcom,wifi-control-func;
	/* Broadcom WiFi solution - End */

	qcom,vdd-io-voltage-level = <1800000 1800000>;
	qcom,vdd-io-current-level = <6 22000>;
	status = "ok";
};

